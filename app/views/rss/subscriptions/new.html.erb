<h1>Create new RSS Subscription</h1>
<div>
  <%= form_with model: @rss_subscription do |form| %>
    <div>
      <%= form.label :rss_url_id, "RSS URL" %>
      <%= form.select :rss_url_id, @urls.map { |url| [url.url, url.id] } %>
    </div>
    <div>
      <%= form.submit %>
    </div>
  <% end %>
  <%= link_to "Cancel", rss_subscriptions_path %>
</div>
